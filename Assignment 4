import java.util.*;

class Main {
    public static void main(String[] args) {
        Circle shape1 = new Circle(0);
        int num1 = shape1.get();
        System.out.println(num1);
        
        Rectangle shape2 = new Rectangle(4);
        int num2 = shape2.get();
        System.out.println(num2);
        shape2.set(6);
        num2 = shape2.get();
        System.out.println(num2);
    }
}

class Shape {
    Integer numOfSides;
    
    public Shape(int sides) {
        numOfSides = sides;
    }
    
    public void set(int num) {
        numOfSides = num;
    }
    
    public int get() {
       System.out.println("Getting sides");
       return numOfSides;
    }
}


class Circle extends Shape {
    public Circle(int sides) {
        super(sides);
    }
}

class Rectangle extends Shape {
    public Rectangle(int sides) {
        super(sides);
    }
    
}
